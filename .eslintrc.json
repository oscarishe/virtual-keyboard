{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base/legacy",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-restricted-syntax": ["error", "ForInStatement", "LabeledStatement", "WithStatement"],
        "no-undef":0
    }
}
